<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" minHeight="400.0" minWidth="400.0" prefHeight="400.0"
            prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121"
            fx:controller="App.ui.TaskInfoController">
    <GridPane prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0"
              AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="189.0" minWidth="10.0" prefWidth="109.0"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="331.0" minWidth="10.0" prefWidth="151.0"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="224.0" minWidth="10.0" prefWidth="223.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Label alignment="CENTER" contentDisplay="CENTER" text="ToDo's info:" GridPane.columnSpan="3"
               GridPane.halignment="CENTER">
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
            <font>
                <Font size="19.0"/>
            </font>
        </Label>
        <Label text="ToDo: " GridPane.halignment="CENTER" GridPane.rowIndex="1">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label text="Owner: " GridPane.halignment="CENTER" GridPane.rowIndex="2">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label text="Deadline: " GridPane.halignment="CENTER" GridPane.rowIndex="3">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label text="Personal: " GridPane.halignment="CENTER" GridPane.rowIndex="4">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label text="Status: " GridPane.halignment="CENTER" GridPane.rowIndex="5">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label text="Priority: " GridPane.halignment="CENTER" GridPane.rowIndex="6">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <HBox GridPane.columnSpan="3" GridPane.rowIndex="8" GridPane.rowSpan="2">
            <children>
                <TableView fx:id="tableExecutors" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                        <TableColumn fx:id="tableColumnExecutors" maxWidth="1.7976931348623157E308" minWidth="-Infinity"
                                     prefWidth="175.0" resizable="false" text="Executors"/>
                    </columns>
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                </TableView>
                <TableView fx:id="notReadyExecutors" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                        <TableColumn fx:id="tableColumnNotReadyExecutors" maxWidth="1.7976931348623157E308"
                                     minWidth="-Infinity" prefWidth="175.0" text="Not ready"/>
                    </columns>
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                </TableView>
            </children>
        </HBox>
        <Separator prefWidth="200.0" GridPane.columnSpan="3" GridPane.rowIndex="7">
            <GridPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </GridPane.margin>
        </Separator>
        <Label fx:id="titleLabel" maxHeight="1.7976931348623157E308" text="Label" GridPane.columnIndex="1"
               GridPane.columnSpan="2" GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1"
               GridPane.vgrow="ALWAYS">
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label fx:id="ownerLabel" text="Label" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="2">
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label fx:id="deadlineLabel" text="Label" GridPane.columnIndex="1" GridPane.columnSpan="2"
               GridPane.rowIndex="3">
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label fx:id="personalLabel" text="Label" GridPane.columnIndex="1" GridPane.columnSpan="2"
               GridPane.rowIndex="4">
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label fx:id="statusLabel" text="Label" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="5">
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label fx:id="prioLabel" text="Label" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="6">
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Button fx:id="btnOK" mnemonicParsing="false" onAction="#buttonClicked" prefWidth="50.0" text="OK"
                GridPane.columnIndex="2" GridPane.rowIndex="10">
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
        </Button>
        <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#buttonClicked" prefWidth="50.0" text="Cancel"
                GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="10">
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
        </Button>
        <Button fx:id="btnEdit" mnemonicParsing="false" onAction="#buttonClicked" prefWidth="50.0" text="Edit..."
                GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="10">
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
        </Button>
    </GridPane>
</AnchorPane>
