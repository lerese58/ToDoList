<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.121"
            fx:controller="App.ui.ProgressController">
    <center>
        <AnchorPane prefHeight="180.0" prefWidth="450.0">
            <children>
                <Label fx:id="labelWait" text="Wait, please ..." AnchorPane.leftAnchor="10.0"
                       AnchorPane.topAnchor="10.0"/>
                <ProgressBar fx:id="progressBar" layoutX="75.0" layoutY="75.0" prefHeight="30.0" prefWidth="300.0"/>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
